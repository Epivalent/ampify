"""
Usage: appengine <command> [options]

Available commands:

   run [dev_appserver.py options]

   prod [dev_appserver.py options]

   deploy [appcfg.py options]
"""

import sys

# ------------------------------------------------------------------------------
# exit if not python 2.5
# ------------------------------------------------------------------------------

if sys.version[:3] != '2.5':
    print "\n!! App Engine needs Python 2.5\n"
    sys.exit(1)
